# 服务器环境配置 - 多账号系统
# 复制到服务器作为 .env 文件

# 多账号管理系统
ENABLE_MULTI_ACCOUNT=true

# 禁用单账号自动化工作线程（避免冲突）
ENABLE_AUTO_WORKER=false

# 自动启用headless模式
AUTOMATION_HEADLESS=true

# 数据库配置
DATABASE_URL=sqlite:///./hailuo_database.db

# 其他配置
JWT_SECRET_KEY=your_secret_key_here_change_this
ADMIN_TOKEN=admin_token_change_this
